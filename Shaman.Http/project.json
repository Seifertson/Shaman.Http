{
  "version": "1.0.1.2",
  "buildOptions": {
    "define": [
      "DESKTOP",
      "STANDALONE",
      "SMALL_LIB_AWDEE",
      "SHAMAN_CORE",
      "NATIVE_HTTP",
      "SHAMAN",
      "SKIP_FORMAT_FUNCTION"
    ],
    "allowUnsafe": true,
    "keyFile": "ShamanOpenSourceKey.snk"
  },
  "authors": [
    "Andrea Martinelli"
  ],
  "description": "HTTP and HTML processing library",
  "iconUrl": "http://shaman.io/images/shaman-nuget-icon.png",
  "projectUrl": "http://shaman.io/",
  "tags": [
    "HTTP",
    "HTML",
    "DOM",
    "JSON"
  ],
  "compile": [],
  "dependencies": {
    "Shaman.BlobStore": "1.0.1.3",
    "Shaman.Fizzler": "1.0.1.10",
    "Shaman.Dom": "1.0.1.17",
    "Shaman.CommonExtensions": "1.0.1.1",
    "Newtonsoft.Json": "6.0.5",
    "Shaman.FileSize": "1.0.1.1",
    "Shaman.FileSystem": "1.0.1.1",
    "Shaman.Configuration.Metadata": "1.0.1.1",
    "Shaman.ValueString": "1.0.2.21"
  },
  "frameworks": {
    "net46": {
      "frameworkAssemblies": {
        "System.Net.Http": "4.1.0"
      }
    },
    "netstandard1.3": {
      "imports": "portable-net45+win8+wp8+wpa81",
      "buildOptions": {
        "define": [
          "CORECLR"
        ]
      },
      "dependencies": {
        "System.Net.Http": "4.1",
        "System.Net.Requests": "4.0.11",
        "System.IO.Compression": "4.1",
        "System.Threading.Timer": "4.0.1",
        "System.Reflection": "4.1",
        "System.Reflection.TypeExtensions": "4.1.0",
        "System.Threading.Thread": "4.0.0",
        "System.Dynamic.Runtime": "4.0.11",
        "System.Text.RegularExpressions": "4.1.0"
      }
    },
    "net35": {
      "compilationOptions": {
        "define": [
          "NET35",
          "DESKTOP",
          "STANDALONE",
          "SMALL_LIB_AWDEE",
          "SHAMAN_CORE",
          "NATIVE_HTTP",
          "WEBCLIENT"
        ]
      },
      "dependencies": {
        "Shaman.AsyncBridge.Extensions": "1.0.0-beta-4642"
      }
    }
  }
}